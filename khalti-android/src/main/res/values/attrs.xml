<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="KhaltiButton">
        <attr name="text" format="string" />
        <attr name="khalti_button_style" format="enum">
            <enum name="basic" value="-1" />
            <enum name="khalti" value="0" />
            <enum name="e_banking" value="1" />
            <enum name="m_banking" value="2" />
            <enum name="sct" value="3" />
            <enum name="connect_ips" value="4" />
        </attr>
    </declare-styleable>

    <declare-styleable name="TextInputLayout">
        <attr name="mainHintTextSize" format="dimension" />
    </declare-styleable>

    <declare-styleable name="ExpandableLayout">
        <attr name="canExpand" format="boolean" />
    </declare-styleable>
</resources>